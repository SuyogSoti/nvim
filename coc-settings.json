{
  "suggest.echodocSupport": true,
  "languageserver": {
    "ciderlsp": {
      "command": "/google/bin/releases/editor-devtools/ciderlsp",
      "args": [
        "--tooltag=coc-nvim",
        "--noforward_sync_responses"
      ],
      "filetypes": [
        "borg",
        "c",
        "cpp",
        "go",
        "java",
        "proto",
        "python",
        "bzl",
        "textproto"
      ]
    }
  },
  "coc.preferences.extensionUpdateCheck": "daily"
}
